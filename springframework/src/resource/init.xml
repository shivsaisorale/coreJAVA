<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"> <!-- bean definitions here -->

	<bean id="charger" class="com.xworkz.spring.Charger">
		<constructor-arg value="poco"></constructor-arg>
		<constructor-arg value="20"></constructor-arg>
		<property name="color" value="red"></property>
		<property name="volt" value="34"></property>
		<property name="working" value="true"></property>

	</bean>


	<bean id="solarwaterheater"
		class="com.xworkz.spring.SolarWaterHeater">
		<property name="capacity" value="20"></property>
		<property name="brand" value="tatasolar"></property>
		<property name="price" value="30000"></property>
		<property name="coldwater" value="false"></property>
	</bean>
	<bean id="ups" class="com.xworkz.spring.Ups">
		<constructor-arg value="luminous"></constructor-arg>
		<constructor-arg value="charging"></constructor-arg>
		<constructor-arg value="30"></constructor-arg>
		<constructor-arg value="2"></constructor-arg>
		<constructor-arg value="5"></constructor-arg>


	</bean>
	<bean id="electric"
		class="com.xworkz.spring.ElectricCookTop">
		<property name="brand" value="hawkins"></property>
		<property name="weight" value="10"></property>
		<property name="color" value="silver"></property>
		<property name="liters" value="2"></property>
		<property name="wireLenght" value="12"></property>
		<property name="customer" value="10"></property>
		<property name="warranty" value="2"></property>
	</bean>
</beans>